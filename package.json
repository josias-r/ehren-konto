{
  "name": "ehren-konto",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prisma:generate": "dotenv -e .env.development.local prisma generate",
    "prisma:studio": "dotenv -e .env.development.local prisma studio",
    "prisma:push": "dotenv -e .env.development.local prisma db push",
    "prisma:pull": "dotenv -e .env.development.local prisma db pull",
    "prisma:save": "dotenv -e .env.development.local prisma migrate save",
    "prisma:reset": "dotenv -e .env.development.local prisma migrate reset",
    "prisma:migrate": "dotenv -e .env.development.local prisma migrate dev --create-only",
    "prisma:deploy": "dotenv -e .env.development.local prisma migrate deploy",
    "build": "npm run prisma:generate && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "@radix-ui/react-avatar": "^1.0.2",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-label": "^2.0.1",
    "@radix-ui/react-navigation-menu": "^1.1.2",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-scroll-area": "^1.0.3",
    "@radix-ui/react-separator": "^1.0.2",
    "@types/node": "20.1.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@vercel/postgres": "^0.3.0",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.1",
    "lucide-react": "^0.216.0",
    "next": "13.4.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.5",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "prisma": "^4.14.0"
  }
